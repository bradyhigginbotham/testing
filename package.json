{
  "name": "app-presto-driver",
  "version": "1.0.0",
  "versionCode": "1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "node ./bin/postInstall",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write '**/*.js'",
    "format:json": "prettier --write '**/*.json'",
    "format:md": "prettier --write '**/*.md'",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 8' --configuration Dev.Debug --scheme \"Presto Driver Dev\"",
    "ios:dev:x": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 11 Pro' --configuration Dev.Debug --scheme \"Presto Driver Dev\"",
    "ios:x": "ENVFILE=.env react-native run-ios --simulator='iPhone XÊ€' --scheme \"Presto Driver Prod\"",
    "ios": "ENVFILE=.env react-native run-ios --simulator \"iPhone 8\" --scheme \"Presto Driver Prod\"",
    "android:dev": "export VERSION_NAME=$npm_package_version && export VERSION_CODE=$npm_package_versionCode && export ENVFILE=.env.dev && react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android": "export VERSION_NAME=$npm_package_version; export VERSION_CODE=$npm_package_versionCode; export ENVFILE=.env react-native run-android --variant='prodDebug'",
    "adb": "adb shell input keyevent 82",
    "reactotron:adb": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/datetimepicker": "^2.5.0",
    "@react-native-community/masked-view": "0.1.5",
    "@react-native-community/netinfo": "^5.9.0",
    "amazon-cognito-identity-js": "^4.3.0",
    "apisauce": "^1.1.1",
    "aws-amplify": "^3.0.12",
    "aws-amplify-react-native": "^4.2.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "events": "^3.0.0",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.6.1",
    "querystringify": "2.1.0",
    "ramda": "^0.26.1",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-animatable": "^1.3.3",
    "react-native-camera": "^3.13.1",
    "react-native-config": "^1.2.1",
    "react-native-device-info": "^5.2.1",
    "react-native-easy-toast": "^1.2.0",
    "react-native-flipper": "^0.45.0",
    "react-native-geolocation-service": "^3.1.0",
    "react-native-gesture-handler": "1.5.0",
    "react-native-google-places": "^3.1.2",
    "react-native-i18n": "2.0.15",
    "react-native-image-crop-picker": "^0.32.0",
    "react-native-image-picker": "^1.1.0",
    "react-native-interactable": "^2.0.1",
    "react-native-intercom": "^13.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-map-link": "^2.5.2",
    "react-native-paper": "^3.10.1",
    "react-native-permissions": "^2.0.2",
    "react-native-progress-circle": "^2.0.1",
    "react-native-reanimated": "1.3.0",
    "react-native-responsive-screen": "https://github.com/emaLorenzo/react-native-responsive-screen.git",
    "react-native-safe-area-context": "0.5.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-simple-popover": "^2.1.0",
    "react-native-snap-carousel": "3.8.0",
    "react-native-status-bar-height": "2.4.0",
    "react-native-user-agent": "^1.0.4",
    "react-native-vector-icons": "6.1.0",
    "react-native-webview": "^7.4.3",
    "react-navigation": "^4.0.10",
    "react-navigation-backhandler": "^1.3.2",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-navigation-stack": "2.0.0-alpha.26",
    "react-navigation-tabs": "2.5.6",
    "react-redux": "^7.2.0",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "1.0.1",
    "seamless-immutable": "^7.1.4",
    "tipsi-stripe": "^7.5.1",
    "validator": "11.1.0",
    "washr-sdk-all": "git+ssh://git@bitbucket.org:hamprdevs/washr-sdk-all.git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/runtime": "^7.9.6",
    "@storybook/addon-storyshots": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/channels": "^4.1.11",
    "@storybook/react-native": "^4.1.11",
    "babel-jest": "^26.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "ignite-andross": "^4.0.3",
    "ignite-animatable": "1.0.2",
    "ignite-i18n": "1.2.0",
    "ignite-redux-persist": "1.1.2",
    "ignite-standard": "1.0.0",
    "ignite-vector-icons": "1.1.1",
    "jest": "^26.0.1",
    "jetifier": "1.6.3",
    "metro-react-native-babel-preset": "^0.58.0",
    "mockery": "^2.1.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.1",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.19.1",
    "react-devtools-core": "4.5.0",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.11.0",
    "rimraf": "3.0.2",
    "snazzy": "^8.0.0",
    "solidarity": "2.3.1",
    "standard": "10.0.2"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ]
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
